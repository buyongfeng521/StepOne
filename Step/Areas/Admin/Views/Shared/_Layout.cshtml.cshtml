    <script type="text/javascript">
        function SetMenu(id, flag) {
            if (flag == 0){
                $(id).addClass("active").siblings().removeClass("active");
            }
            else if (flag == 1) {
                $(id).addClass("active").siblings().removeClass("active");
                $(id).parent().css("display", "block").parent().addClass("open").siblings().removeClass("active");
            }
        };
